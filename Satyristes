;; For Satyrographos 0.0.2 series
(version 0.0.2)

(library
  (name "ruby")
  (version "0.1.0")
  (sources
    (
     (package "ruby.satyh" "./ruby.satyh")
    )
  )
  (opam "satysfi-ruby.opam")
  (dependencies ()))

(libraryDoc
  (name "ruby-doc")
  (version "0.1.0")
  (workingDirectory "doc")
  (build
    ((satysfi "ruby-ja.saty" "-o" "ruby-ja.pdf")))
  (sources
    ((doc "ruby-ja.pdf" "doc/ruby-ja.pdf")))
  (opam "satysfi-ruby-doc.opam"))